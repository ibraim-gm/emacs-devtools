(:name auto-complete-mode
 :type :git
 :url "git://github.com/auto-complete/auto-complete.git"
 :build (("make byte-compile"))
 :load-path ("." "lib/popup")
 :features (auto-complete auto-complete-config)
 :after (progn
          (add-to-list
           'ac-dictionary-directories
           (expand-file-name (concat default-directory "/dict")))))
