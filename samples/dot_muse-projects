(setq muse-project-alist
      `(("name-of-my-project" (,@(muse-project-alist-dirs "~/source/of/my/project") :default "index")
         (:base "html" :path "~/public_html")
         (:base "pdf" :path "~/public_html/pdf")
         ;; add more output options here
         )))
